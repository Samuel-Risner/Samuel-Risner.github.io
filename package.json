{
  "name": "samuel-risner.github.io",
  "version": "1.0.0",
  "description": "The branch for creating my github.io page. (NOT for hosting it)",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "npm run build && npm run start",
    "build": "npm run build_ts && npm run build_tw",
    "build_ts": "npm run build_ts_server && npm run build_ts_static",
    "build_ts_server": "tsc -p ./tsconfig/server.json",
    "build_ts_static": "tsc -p ./tsconfig/static.json",
    "build_tw": "tailwindcss -i ./tw/input.css -o ./static/css/dist/output.css",
    "watch_tw": "tailwindcss -i ./tw/input.css -o ./static/css/dist/output.css --watch",
    "render": "node ./dist/render_templates.js",
    "move": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./move.ps1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Samuel-Risner/Samuel-Risner.github.io.git#create"
  },
  "author": "Samuel Risner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Samuel-Risner/Samuel-Risner.github.io/issues"
  },
  "homepage": "https://github.com/Samuel-Risner/Samuel-Risner.github.io/tree/create#readme",
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/nunjucks": "^3.2.2",
    "express": "^4.18.2",
    "nunjucks": "^3.2.3",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5"
  }
}
